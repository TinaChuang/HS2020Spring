  <!-- Modal -->
<div class="modal right fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog m-0">
    <div class="modal-content rounded-0 py-4 px-5">
      <div class="modal-header border-0">
        <h5 class="modal-title h3 font-weight-bold" id="exampleModalLabel">
          Cart List
          <span class="align-text-top text-primary text-small">3</span>
        </h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span class="material-icons">clear</span>
        </button>
      </div>
      <div class="modal-body ">
        <div class="cartList mb-4">
          <div class="d-flex itemCard mb-2">
            <img class="img-left img-fluid" src="https://images.unsplash.com/photo-1559810087-893a00a35608?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=750&q=80" alt="">
            <div class="w-100 bg-light p-3 position-relative">
              <h4 class="font-weight-bold">Tatami Cup</h4>
              <p class="text-secondary">
                NT$1,080
                <span class="text-line-through text-small">NT$1,200</span>
              </p>
              <div class="d-flex justify-content-between align-items-center">
                <div class="prod-quantity bg-light">
                  <button class="btn rounded-left">
                    <span class="material-icons align-middle">remove</span>
                    
                  </button>
                  <span class="prod-quantity-num px-1">1</span>
                  <button class="btn rounded-right">
                    <span class="material-icons align-middle">add</span>
                  </button>
                </div>
                <p class="font-weight-bold m-1">NT$1,080</p>
              </div>
              <button type="button" class="close position-absolute m-3">
                <span class="material-icons">clear</span>
              </button>
            </div>
          </div>
          <div class="d-flex itemCard mb-2">
            <img class="img-left img-fluid" src="https://images.unsplash.com/photo-1559810087-893a00a35608?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=750&q=80" alt="">
            <div class="w-100 bg-light p-3 position-relative">
              <h4 class="font-weight-bold">Tatami Cup</h4>
              <p class="text-secondary">
                NT$1,080
                <span class="text-line-through text-small">NT$1,200</span>
              </p>
              <div class="d-flex justify-content-between align-items-center">
                <div class="prod-quantity bg-light">
                  <button class="btn rounded-left">
                    <span class="material-icons align-middle">remove</span>
                    
                  </button>
                  <span class="prod-quantity-num px-1">1</span>
                  <button class="btn rounded-right">
                    <span class="material-icons align-middle">add</span>
                  </button>
                </div>
                <p class="font-weight-bold m-1">NT$1,080</p>
              </div>
              <button type="button" class="close position-absolute m-3">
                <span class="material-icons">clear</span>
              </button>
            </div>
          </div>
        </div>        
        <div class="priceArea d-flex">
          <div class="emptyBlock"></div>
          <div class="d-flex flex-column w-100">
            <p class="d-flex justify-content-between">
              <span>Subtotal</span>
              <span>NT$4,680</span>
            </p>
            <p class="d-flex justify-content-between">
              <span>Shipping</span>
              <span>NT$80</span>
            </p>
            <p class="d-flex justify-content-between font-weight-bold mb-4 h4">
              <span>Total</span>
              <span>NT$4,760</span>
            </p>
            <button type="button" class="btn btn-primary py-3">Checkout</button>
          </div>
          
        </div>
      </div>
    </div>
  </div>
